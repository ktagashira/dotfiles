Host mosin.jp
    HostName ssh.mosin.jp
    User mosin
    IdentityFile ~/.ssh/priv_keys/koetsuki.pem
    Port 22
    TCPKeepAlive yes

Host github.com
    HostName github.com
    IdentityFile ~/.ssh/priv_keys/id_rsa

Host *
    ControlMaster auto
    ControlPath ~/.ssh/control/%C
    ControlPersist 120m

    StrictHostKeyChecking no
    ForwardAgent yes
    AddKeysToAgent yes
    UseKeychain yes
    IdentitiesOnly yes
